# Ignore Python bytecode and build artifacts
__pycache__/
*.py[cod]
*$py.class

# Ignore Python packaging artifacts
build/
dist/
sdist/
wheels/
*.egg-info/

# Ignore virtual environments
.venv/
env/
venv/
ENV/

# Ignore IDE/editor files (vscode, pycharm, etc.)
.vscode/
.idea/
.spyderproject
.ropeproject

# Ignore log files
*.log
log.txt

# Ignore test directories and files (unless they are needed for Docker)
test/
*test.py

# Ignore temporary files or environment-specific files
*.old
.vim/
*.session

# Ignore package manager files that aren't needed for Docker
Pipfile.lock
poetry.lock
pdm.lock

# Ignore documentation build artifacts
docs/_build/

# Ignore pyenv versions and similar
.python-version

# Ignore environment files (these should not be included in the Docker context)
.env
